<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>run</actionName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles> 
        <properties>
            <jalopy.skip>true</jalopy.skip>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>-Ddirectory.extension=Belis2 -Xdebug -Xrunjdwp:transport=dt_socket,server=y,address=8990,suspend=n -Xms256m -Xmx1024m -Duser.country=DE -classpath ${de.cismet.cids.classpath} -Duser.language=de -Djnlp.callserverhost=http://localhost:9917/callserver/binary -Djnlp.domain=BELIS2 de.cismet.belis.client.BelisClient</exec.args>
            <exec.executable>java</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>debug</actionName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
        <properties>
            <jalopy.skip>true</jalopy.skip>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>-Ddirectory.extension=2 -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xms256m -Xmx1024m -Duser.country=DE -Duser.language=de -classpath ${de.cismet.cids.classpath} de.cismet.belis.client.BelisClient</exec.args>
            <jpda.listen>true</jpda.listen>
            <exec.executable>java</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>profile</actionName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
        <properties>
            <exec.args>${profiler.args} -Ddirectory.extension=2 -Xms256m -Xmx1024m -Duser.country=DE -Duser.language=de -classpath ${de.cismet.cids.classpath} de.cismet.belis.client.BelisClient</exec.args>
            <profiler.action>profile</profiler.action>
            <exec.executable>${profiler.java}</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>debug.single.main</actionName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.2:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
        <properties>
            <jalopy.skip>true</jalopy.skip>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <jpda.listen>true</jpda.listen>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath ${de.cismet.cids.classpath} ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>run.single.main</actionName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.2:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
        <properties>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <exec.args>-classpath ${de.cismet.cids.classpath} ${packageClassName}</exec.args>
            <jalopy.skip>true</jalopy.skip>
            <exec.executable>java</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>build</actionName>
        <goals>
            <goal>install</goal>
        </goals>
        <properties>
            
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-deploy</actionName>
        <displayName>deploy</displayName>
        <goals>
            <goal>deploy</goal>
        </goals>
    </action>
</actions>
